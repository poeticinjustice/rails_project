<h1>Get that job!</h1>

<%= link_to 'New post', new_article_path %>

<table>
  <tr>
    <th>Title: </th>
    <th>link: </th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.link %></td>
      <td><%= link_to 'Show', article_path(article) %></td>
    </tr>
  <% end %>
</table>
